<?xml version="1.0" ?>
<config>
	<!-- Global simulation parameters -->
	<output resultsFile="${output}" logsDir="${logs}"/>
	<numSteps value="${timesteps}" serializeResolution="1"/>
	<size width="50" height="50"/>
    <environment map="${map}"/>

    <resource consumptionFactor="${consumption_factor}" />

    <!-- Agent configuration parameters -->
    <agents reproduction="${agent_reproduction}" position="${agent_position}">
        ${agent_config}
    </agents>
</config>

